{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google",
        "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
      "object-curly-spacing": ["error", "always"],
      "space-in-parens": ["error", "always"],
      "max-len": 0,
      "camelcase": 0

    }
}
